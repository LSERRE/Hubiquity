<% @user_reviews.each do |review| %>
	<%= review.visiter.visit.adress %> <%= review.visiter.visit.city %> <%= review.visiter.visit.zipcode %>
	By <%= review.visiter.visiter.name %>
	<%= image_tag(review.visiter.visiter.avatar.url(:thumb), :class => "content avatar") %>
	<%= image_tag(review.picture1, :class => "LACLASSEQUETUVEUX") %>
<% end %>